<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag 'contacts' %>
  </head>
  <body>
    <div class="container">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>">
          <%= value %>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
